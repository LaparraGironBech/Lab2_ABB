<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - ListaArtesanal</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />

</head>
<body>

    <div style="width: 100%;height: 400px;background-image: url(https://www.cotecnova.edu.co/wp-content/uploads/2016/06/banner-REGENCIA-EN-FARMACIA.jpg);background-repeat:no-repeat;"></div>

    <div style="margin: 3% 3% 7% 7%" class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>

    </div>

    <footer style="background: #3a5560" class="border-top footer text-muted">
        <div class="container">
            <font color="#fff ">   &copy; 2021 - ListaArtesanal - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a></font>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/js/html2pdf.bundle.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
